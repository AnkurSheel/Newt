{
    "version": "1.1.1",
    "name": "Newt",
    "main": "./dist/main.bundle.js",
    "author": "Ankur Sheel <ankur.sheel@gmail.com>",
    "license": "MIT",
    "description": "A app to view networth changes",
    "private": true,
    "repository": {
        "type": "git",
        "url": "https://github.com/AnkurSheel/Poseidon.git"
    },
    "keywords": [
        "electron",
        "typescript",
        "react",
        "webpack",
        "sqlite3"
    ],
    "scripts": {
        "start": "electron .",
        "watch": "env-cmd .env.dev webpack-dev-server --port 3000 --config ./config --progress --colours --remote-debugging-port=9223",
        "dev": "rimraf dist && env-cmd .env.dev webpack --config ./config --progress --colours",
        "prod": "rimraf dist && env-cmd .env webpack --config ./config",
        "postinstall": "electron-builder install-app-deps",
        "build": "yarn prod &&  electron-builder . --config ./electron-builder.json --publish never",
        "semantic-release": "semantic-release",
        "commit": "yarn git-cz"
    },
    "husky": {
        "hooks": {
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
    },
    "build": {
        "npmRebuild": "true"
    },
    "dependencies": {
        "@date-io/moment": "1.1.0",
        "@material-ui/core": "3.9.3",
        "electron-devtools-installer": "2.2.4",
        "electron-log": "3.0.5",
        "electron-updater": "4.0.6",
        "knex": "0.15.2",
        "material-ui-pickers": "2.2.4",
        "moment": "2.24.0",
        "react": "16.8.2",
        "react-dom": "16.8.2",
        "react-router-dom": "5.0.0",
        "recharts": "1.5.0",
        "recompose": "0.30.0",
        "sqlite3": "4.0.6"
    },
    "devDependencies": {
        "@commitlint/cli": "7.5.2",
        "@commitlint/config-conventional": "7.5.0",
        "@semantic-release/changelog": "3.0.2",
        "@semantic-release/exec": "3.3.2",
        "@semantic-release/git": "7.0.8",
        "@types/electron-devtools-installer": "2.2.0",
        "@types/knex": "0.15.2",
        "@types/react": "16.8.3",
        "@types/react-dom": "16.8.2",
        "@types/react-router-dom": "4.3.1",
        "@types/recharts": "1.1.14",
        "@types/recompose": "0.30.5",
        "@types/sqlite3": "3.1.5",
        "commitizen": "3.0.7",
        "css-loader": "2.1.0",
        "cz-conventional-changelog": "2.1.0",
        "electron": "4.0.3",
        "electron-builder": "20.38.5",
        "emoji-cz": "0.3.0",
        "env-cmd": "8.0.2",
        "faker": "4.1.0",
        "html-webpack-plugin": "3.2.0",
        "husky": "1.3.1",
        "mini-css-extract-plugin": "0.5.0",
        "node-sass": "4.11.0",
        "optimize-css-assets-webpack-plugin": "5.0.1",
        "react-hot-loader": "4.8.0",
        "rimraf": "2.6.3",
        "sass-loader": "7.1.0",
        "semantic-release": "15.13.3",
        "source-map-loader": "0.2.4",
        "style-loader": "0.23.1",
        "ts-loader": "5.3.3",
        "tslint": "5.12.1",
        "typescript": "3.3.3",
        "webpack": "4.29.6",
        "webpack-cli": "3.2.3",
        "webpack-dev-server": "3.2.1",
        "webpack-merge": "4.2.1",
        "webpack-notifier": "1.7.0"
    }
}
