{
    "version": "0.0.0",
    "name": "Newt",
    "main": "./dist/main.bundle.js",
    "author": "Ankur Sheel <ankur.sheel@gmail.com>",
    "license": "MIT",
    "description": "A app to view networth changes",
    "private": true,
    "repository": {
        "type": "git",
        "url": "https://github.com/AnkurSheel/Poseidon.git"
    },
    "keywords": [
        "electron",
        "typescript",
        "react",
        "webpack",
        "sqlite3"
    ],
    "scripts": {
        "start": "electron .",
        "watch": "webpack-dev-server --port 3000 --config ./config/webpack.dev.js --progress --colours --remote-debugging-port=9223",
        "dev": "rimraf dist && webpack --config ./config/webpack.dev.js --progress --colours",
        "prod": "rimraf dist && webpack --config ./config/webpack.prod.js",
        "build": "yarn prod && electron-builder . --config ./electron-builder.json",
        "postinstall": "electron-builder install-app-deps",
        "buildNoPublish": "yarn prod &&  electron-builder . --config ./electron-builder.json --publish never",
        "semantic-release": "semantic-release",
        "commit": "yarn git-cz"
    },
    "build": {
        "npmRebuild": "true"
    },
    "dependencies": {
        "knex": "0.15.2",
        "moment": "^2.24.0",
        "react": "^16.8.2",
        "react-dom": "^16.8.2",
        "react-router-dom": "^5.0.0",
        "react-table": "^6.9.2",
        "recharts": "^1.5.0",
        "sqlite3": "^4.0.6"
    },
    "devDependencies": {
        "@semantic-release/changelog": "^3.0.2",
        "@semantic-release/git": "^7.0.8",
        "@types/electron-devtools-installer": "^2.2.0",
        "@types/knex": "^0.15.2",
        "@types/react": "^16.8.3",
        "@types/react-dom": "^16.8.2",
        "@types/react-router-dom": "^4.3.1",
        "@types/react-table": "^6.8.0",
        "@types/recharts": "^1.1.14",
        "@types/recompose": "^0.30.5",
        "@types/sqlite3": "^3.1.5",
        "commitizen": "^3.0.7",
        "css-loader": "^2.1.0",
        "cz-conventional-changelog": "2.1.0",
        "electron": "4.0.3",
        "electron-builder": "^20.38.5",
        "electron-devtools-installer": "^2.2.4",
        "faker": "^4.1.0",
        "html-webpack-plugin": "^3.2.0",
        "mini-css-extract-plugin": "^0.5.0",
        "node-sass": "^4.11.0",
        "optimize-css-assets-webpack-plugin": "^5.0.1",
        "react-hot-loader": "^4.8.0",
        "recompose": "^0.30.0",
        "rimraf": "^2.6.3",
        "sass-loader": "^7.1.0",
        "semantic-release": "^15.13.3",
        "source-map-loader": "^0.2.4",
        "style-loader": "^0.23.1",
        "ts-loader": "^5.3.3",
        "tslint": "^5.12.1",
        "typescript": "^3.3.3",
        "webpack": "^4.29.6",
        "webpack-cli": "^3.2.3",
        "webpack-dev-server": "^3.2.1",
        "webpack-merge": "^4.2.1",
        "webpack-notifier": "^1.7.0"
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog"
        }
    }
}
